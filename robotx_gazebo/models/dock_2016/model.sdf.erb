
<?xml version="1.0" ?>
<sdf version="1.6">
  <model name="robotx_dock_2016">
    <% 
    layout = [
      'XXXX      XXXX      XXXX      XXXX',
      'XXXX      XXXX      XXXX      XXXX',
      'XXXX      XXXX      XXXX      XXXX',
      'XXXX      XXXX      XXXX      XXXX',
      'XXXX      XXXX      XXXX      XXXX',
      'XXXX      XXXX      XXXX      XXXX',
      'XXXX      XXXX      XXXX      XXXX',
      'XXXX      XXXX      XXXX      XXXX',
      'XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX',
      'XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX',
      'XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX',
      'XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX',
    ]
    %>
    <!-- 
    <% layout.each do |row| -%>  
      <%= row -%>
    <% end %>
    -->
    <%- x_counter = 0 -%>
    <%- layout.each do |row| %>
    <!-- Row -->
      <%- y_counter=0 -%>
      <%- for j in 0..row.size - 1 -%>
        <%- c = row[j] -%>
        <%- if c == 'X' -%>
    <include>
      <pose><%= x_counter %> <%= y_counter %> 0.25 0 0 0</pose>
      <uri>model://dock_block</uri>
    </include>
        <%- else -%>
        <%- end -%>      
        <%- y_counter += 0.5 -%>
      <%- end -%>
      <% x_counter += 0.5 -%>
    <% end -%>

  </model>
</sdf>
